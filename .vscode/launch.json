{
	"version": "0.1.0",
	"configurations": [
		{
			"name": "Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}"
			],
			"outFiles": [
				"${workspaceFolder}/out/**/*.js"
			],
			"preLaunchTask": "npm: watch",
			"skipFiles": [
				"${workspaceFolder}/node_modules/**/*.js",
				"<node_internals>/**/*.js"
			]
		}
	]
}
